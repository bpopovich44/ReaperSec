    <connection>epiphany_db</connection>
    <sql>SELECT sql1.date, sql1.Username, sql1.Placement, sql2.Opportunities, sql1.Impressions,  sql1.FinalPublisherNetRev FROM 
(SELECT
    f.date, 
    p.name AS Username, 
    i.name AS Placement,
    SUM(f.adImps) AS Impressions, 
    SUM(f.finalRev_ToPub) AS FinalPublisherNetRev, 
    CONCAT(f.date,i.name,p.name) AS multiAlias 
FROM 
    `test_Final` f 
    LEFT OUTER JOIN `inventorySources` i ON i.id = f.invSourceID 
    LEFT OUTER JOIN `publishers` p ON p.id = i.publisherID 
    LEFT OUTER JOIN `markets` m ON m.id = f.marketID 
WHERE 
    f.date BETWEEN '2017-12-01' AND '2017-12-31' AND i.isPlatformTag = 0 AND p.id = ?
GROUP BY 
    f.date, p.id, i.id  
ORDER BY 
    p.name, i.name) AS sql1 

LEFT JOIN 
(SELECT 
    a.date, 
    SUM(a.adOpps) AS Opportunities, 
    CONCAT(a.date,i.name,p.name) AS multiAlias
FROM
    `test_Aggregate` a 
    LEFT OUTER JOIN `inventorySources` i ON i.id = a.invSourceID 
    LEFT OUTER JOIN `publishers` p ON p.id = i.publisherID 
WHERE 
    a.date BETWEEN '2017-12-01' AND '2017-12-31' AND i.isPlatformTag = 0 AND p.id = ?
GROUP BY 
    a.date, p.id, i.id 
ORDER BY 
    p.name, i.name) AS sql2 

ON sql1.multiAlias = sql2.multiAlias

UNION

SELECT sql1.date, sql1.Username, sql1.Placement, sql2.Opportunities, sql1.Impressions, sql1.FinalPublisherNetRev FROM 
(SELECT
    f.date, 
    p.name AS Username, 
    itags.name AS Placement,
    SUM(f.adImps) AS Impressions, 
    SUM(f.finalRev_ToPub) AS FinalPublisherNetRev, 
    CONCAT(f.date,itags.name,p.name) AS multiAlias 
FROM 
    `test_Final` f 
    LEFT OUTER JOIN `inventorySources` i ON i.id = f.invSourceID 
    LEFT OUTER JOIN `intelligentTags` itags ON itags.id = i.intelligentTagID 
    LEFT OUTER JOIN `publishers` p ON p.id = i.publisherID 
    LEFT OUTER JOIN `markets` m ON m.id = f.marketID 
WHERE 
    f.date BETWEEN '2017-12-01' AND '2017-12-31' AND i.isPlatformTag = 1 AND p.id = ?
GROUP BY 
    f.date, p.id, itags.id 
ORDER BY 
    p.name, itags.name) AS sql1 

LEFT JOIN 
(SELECT 
    a.date, 
    SUM(a.adOpps) AS Opportunities, 
    CONCAT(a.date,itags.name,p.name) AS multiAlias
FROM
    `test_Aggregate` a 
    LEFT OUTER JOIN `inventorySources` i ON i.id = a.invSourceID 
    LEFT OUTER JOIN `intelligentTags` itags ON itags.id = i.intelligentTagID 
    LEFT OUTER JOIN `publishers` p ON p.id = i.publisherID 
WHERE 
    a.date BETWEEN '2017-12-01' AND '2017-12-31' AND i.isPlatformTag = 1 AND p.id = ?
GROUP BY 
    a.date, p.id, itags.id 
ORDER BY 
    p.name, itags.name) AS sql2 

ON sql1.multiAlias = sql2.multiAlias ORDER BY DATE ASC</sql>
