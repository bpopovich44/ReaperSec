  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>epiphany_db</connection>
    <sql>SELECT sql1.DATE, sql2.ADTRANS_Opportunities, sql2.ADTRANS_Impressions, sql2.ADTRANS_FillRate, sql1.ADTRANS_Revenue_Before_Fees, sql1.ADTRANS_Fee_Servingcost, sql1.ADTRANS_Fee_Clearinghouse,
		sql1.ADTRANS_Fee_Platform, sql2.ADTRANS_Trashfee,  (sql1.ADTRANS_Revenue_Before_Fees - sql1.ADTRANS_Fee_Servingcost - sql1.ADTRANS_Fee_Clearinghouse - sql1.ADTRANS_Fee_Platform
		- sql2.ADTRANS_TrashFee) AS ADTRANS_Revenue_After_Fees FROM
(SELECT 
	f.date AS DATE,
	SUM(f.adRevenue) AS ADTRANS_Revenue_Before_Fees,
	SUM(f.servingCost) AS ADTRANS_Fee_Servingcost,
	SUM(f.fee_clearingHouse) AS ADTRANS_Fee_Clearinghouse,
	SUM(f.fee_platform) AS ADTRANS_Fee_Platform

FROM
	test_Final AS f
	
WHERE f.`date` BETWEEN ? AND ? AND f.marketID IN (34, 35)
GROUP BY DATE
) AS sql1

LEFT JOIN

(SELECT
	r.date AS DATE,
	SUM(r.adOpps) AS ADTRANS_Opportunities,
	SUM(r.adImps) AS ADTRANS_Impressions,
	SUM(r.adImps) / SUM(r.adOpps) AS ADTRANS_FillRate,
	SUM(r.adOpps / 1000 * .0004) - SUM(r.adImps /1000 * .05) AS ADTRANS_TrashFee
	
FROM
	reports_InventorySource r
	
	
WHERE r.date BETWEEN ? AND ? AND platformID = 6
GROUP BY DATE
) AS sql2

ON sql1.date=sql2.date

UNION ALL 

SELECT sql1.DATE, sql2.ADTRANS_Opportunities, sql2.ADTRANS_Impressions, sql2.ADTRANS_FillRate, sql1.ADTRANS_Revenue_Before_Fees, sql1.ADTRANS_Fee_Servingcost, sql1.ADTRANS_Fee_Clearinghouse,
		sql1.ADTRANS_Fee_Platform, sql2.ADTRANS_Trashfee,  (sql1.ADTRANS_Revenue_Before_Fees - sql1.ADTRANS_Fee_Servingcost - sql1.ADTRANS_Fee_Clearinghouse - sql1.ADTRANS_Fee_Platform
		- sql2.ADTRANS_TrashFee) AS ADTRANS_Revenue_After_Fees FROM
(SELECT 
	f.date AS DATE,
	SUM(f.adRevenue) AS ADTRANS_Revenue_Before_Fees,
	SUM(f.servingCost) AS ADTRANS_Fee_Servingcost,
	SUM(f.fee_clearingHouse) AS ADTRANS_Fee_Clearinghouse,
	SUM(f.fee_platform) AS ADTRANS_Fee_Platform
	
FROM
	test_Final AS f
	
WHERE f.`date` BETWEEN ? AND ? AND f.marketID IN (34, 45)

) AS sql1

LEFT JOIN

(SELECT
	r.date AS DATE,
	SUM(r.adOpps) AS ADTRANS_Opportunities,
	SUM(r.adImps) AS ADTRANS_Impressions,
	SUM(r.adImps) / SUM(r.adOpps) AS ADTRANS_FillRate,
	SUM(r.adOpps / 1000 * .0004) - SUM(r.adImps /1000 * .05) AS ADTRANS_TrashFee
	
	
FROM
	reports_InventorySource r
	
WHERE r.date BETWEEN ? AND ? AND platformID = 6

) AS sql2

ON sql1.DATE = sql2.DATE 

</sql>
